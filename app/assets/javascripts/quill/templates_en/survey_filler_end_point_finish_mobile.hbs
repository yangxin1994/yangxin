<div class="page">
	<div class="free_describe">
		<div class="answer_finish">
			<b><i></i>Finished!</b>
			<div>You have gained {{reward_point}} points.</div>
		</div>
	</div>
	<div class="free_form">
    {{#if signin}}
		  <button class="rew_next" id="rew_next" >View my points</button>
    {{else}}
      <button class="rew_next" id="rew_next" >Sign in</button>
    {{/if}}
    {{#if spreadable}}
      <button id="socal_share">Share to get {{spread_point}} more points</button>
    {{else}}
      <button id="socal_share">Share survey</button>
    {{/if}}
	</div>		
</div>