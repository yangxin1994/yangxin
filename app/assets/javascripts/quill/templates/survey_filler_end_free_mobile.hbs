<div class="page">
	<div class="free_describe">
		<span>
			<b>答题完成啦！</b>
		</span>
	</div>
	<div class='free_form'>
		{{#unless agent}}
			{{#if link}}
				{{#if is_iqiyi}}
					<button id='close_btn'>领取答题奖励</button>
				{{else}}
					<button id='close_btn'>关闭窗口</button>
				{{/if}}
				
				}
			{{else}}
				{{#if spreadable}}
					<button id="socal_share" class="single">分享问卷得{{spread_point}}积分</button>
				{{else}}
		  			<button id="socal_share" class="single">分享问卷</button>
				{{/if}}
			{{/if}}

		{{/unless}}
	</div>	
</div>
<div class="footer">
	{{#if allow_pageup}}	
	<a href="javascript:void(0)" class="prev" id="check_btn" title="返回检查答案"></a>
	{{/if}}	
	<i class="footer-divide"></i>
	{{#if show_restart}}
	<a href="javascript:void(0)" class="resume" id="restart_btn">重新答题</a>
	{{else}}
		{{#if allow_replay}}
			<a href="javascript:void(0)" class="resume" id="replay">重新答题</a>
		{{/if}}
		{{#if allow_multianswer}}
			{{#if signin}}
				<a class='signout-btn' href='javascript:void(0);'>再答一份</a>
			{{else}}
				<button class='newanswer-btn'>再答一份</button>
			{{/if}}
		{{/if}}		
	{{/if}}
</div>