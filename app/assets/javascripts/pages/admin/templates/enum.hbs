<div class="enum-panel panel"> <!-- enum or array view -->
  <table cellpadding="0" cellspacing="0" width="100%" class="table table-striped">
    <thead>
      <tr>
        <td>选项</td>
        <td>绑定属性</td>
      </tr>
    </thead>
    <tbody>
      {{#each question.issue.items}}
      <tr>
        <td data-id="{{id}}">{{content.text}}</td>
        <td>
          <select id="" name="">
            <option value=""></option>
            {{#each ../attr.enum_array}}
            <option value="{{@index}}">{{this}}</option>
            {{/each}}
          </select>
        </td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>
